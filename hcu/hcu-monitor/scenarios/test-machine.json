{
    "title": "Testing GridSim Monitoring Simulation",
    "monitoring_agents":
    [
        {
            "name": "csv_agent",
            "class": "at.ac.tuwien.dsg.hcu.monitor.impl.agent.BaseMonitoringAgent",
            "adapter":
            {
                "class": "at.ac.tuwien.dsg.hcu.monitor.impl.adapter.CSVMonitoringAdapter",
                "config":
                {
                    "csv_file": "data/4.2.2015/10.99.0.18.csv",
                    "csv_time_cfg":
                    {
                        "csv_col": "Time ",
                        "offset": -1422885048,
                        "scale": 5.43,
                        "function": "at.ac.tuwien.dsg.hcu.monitor.util.Converter.perMil"
                    }
                }
            },
            "producer":
            {
                "class": "at.ac.tuwien.dsg.hcu.monitor.impl.producer.BaseMonitoringProducer",
                "config": {}
            },
            "topics":
            [
                {
                    "name": "cpu_idle",
                    "config":
                    {
                        "csv_col": "cpu_idle "
                    }
                },
                {
                    "name": "utilization",
                    "config":
                    {
                        "csv_col": "cpu_idle ",
                        "function" : "at.ac.tuwien.dsg.hcu.monitor.util.Converter.idleToUtil"
                    }
                }
            ]
        },
        {
            "enabled": true,
            "name": "event_based_agent",
            "class": "at.ac.tuwien.dsg.hcu.monitor.impl.agent.BaseMonitoringAgent",
            "consumer":
            {
                "class": "at.ac.tuwien.dsg.hcu.monitor.impl.consumer.EventBasedMonitoringConsumer",
                "subscriptions":
                [
                    {
                        "to": "csv_agent",
                        "topic": "utilization",
                        "config": {}
                    }
                ],
                "config": {}
            },
            "producer":
            {
                "class": "at.ac.tuwien.dsg.hcu.monitor.impl.producer.BaseMonitoringProducer",
                "config": {}
            },
            "topics":
            [
                {
                    "name": "avg_util",
                    "config":
                    {
                        "event_processor": "at.ac.tuwien.dsg.hcu.monitor.impl.processor.UtilizationProcessor",
                        "args": {
                            "aggregate": "avg"
                        }
                    }
                }
            ]
        },
        {
            "name": "csv_consumer",
            "class": "at.ac.tuwien.dsg.hcu.monitor.impl.agent.BaseMonitoringAgent",
            "consumer":
            {
                "class": "at.ac.tuwien.dsg.hcu.monitor.impl.consumer.DumpMonitoringConsumer",
                "subscriptions":
                [
                    {
                        "to": "csv_agent",
                        "topic": "utilization",
                        "config": {},
                        "_quality": {
                            "rate": 10000.0
                        }
                    },
                    {
                        "to": "event_based_agent",
                        "topic": "avg_util",
                        "config": {},
                        "_quality": {
                            "accuracy": 0.5
                        }
                    }
                ],
                "config": {}
            }
        }
    ]
}